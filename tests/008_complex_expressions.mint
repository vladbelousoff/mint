// Arithmetic operators
let a = 10 + 20 - 5 * 3 / 2;

// Assignment operators
let x = 5;
x += 10;
x -= 3;
x *= 2;
x /= 4;

// Comparison operators
let isGreater = a > b;
let isLess = a < b;
let isEqual = a == b;
let isNotEqual = a != b;
let isGreaterEqual = a >= b;
let isLessEqual = a <= b;

// Complex expressions with parentheses
let complex = (a + b) * (c - d) / (e + f);

// Nested expressions
let nested = ((a + b) * (c - d)) / e;

// Expression with member access
let result = object.property + array[index].field * 2; 